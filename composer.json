{
    "name": "tarsana/functional",
    "description": "functional programming library for Tarsana",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Amine Ben hammou",
            "email": "webneat@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Tarsana\\Functional\\": "src/"
        },
        "files": [
          "src/functions.php",
          "src/operators.php",
          "src/string.php",
          "src/array.php",
          "src/math.php"
        ]
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/matteosister/php-curry"
        }
    ],
    "require": {
        "cypresslab/php-curry": "^0.4"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    }
}
